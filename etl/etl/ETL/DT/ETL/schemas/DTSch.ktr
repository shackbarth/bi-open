    <sql>DROP VIEW IF EXISTS &quot;ERPI&quot;.&quot;FISCAL_PERIODS_CL&quot;;
DROP VIEW IF EXISTS &quot;ERPI&quot;.&quot;PERIODS_CL&quot;;
DROP VIEW IF EXISTS &quot;ERPI&quot;.&quot;DATE_DATA_CL&quot;;
DROP TABLE IF EXISTS &quot;ERPI&quot;.&quot;DATE_DATA&quot; CASCADE;
DROP TABLE IF EXISTS &quot;ERPI&quot;.&quot;DATE_DATA_EXTRACT&quot; CASCADE;
DROP TABLE IF EXISTS &quot;ERPI&quot;.&quot;FISCAL_PERIODS&quot; CASCADE;
DROP TABLE IF EXISTS &quot;ERPI&quot;.&quot;PERIODS&quot; CASCADE;  
DROP TABLE IF EXISTS &quot;ERPI&quot;.&quot;TEMP_FISCAL_PERIODS&quot; CASCADE;
DROP TABLE IF EXISTS &quot;ERPI&quot;.&quot;TEMP_PERIODS&quot; CASCADE;

--
--  DATE_DATA is the time dimension at date level for transactional cubes
--
CREATE TABLE &quot;ERPI&quot;.&quot;DATE_DATA&quot; 
(
	&quot;TENANT&quot;		VARCHAR(128) NOT NULL DEFAULT &apos;not used&apos;,
    &quot;DATE_PATTERN&quot; 	VARCHAR(2) NOT NULL,
    &quot;DAYS_DATE&quot; 	INTEGER NOT NULL,
    &quot;THE_DATE&quot; 		DATE,
    &quot;YEAR&quot; 			SMALLINT,
    &quot;QUARTER&quot; 		SMALLINT,
    &quot;PERIOD&quot;		SMALLINT,
    &quot;MONTH&quot;			SMALLINT,
    &quot;WEEK&quot;			SMALLINT,
    &quot;DAY&quot; 			SMALLINT
);

CREATE INDEX &quot;DATE_DATA_PK_INDEX&quot;
   ON &quot;ERPI&quot;.&quot;DATE_DATA&quot;
   (
		&quot;TENANT&quot;,
		&quot;DATE_PATTERN&quot;,
		&quot;DAYS_DATE&quot;
   );

CREATE INDEX &quot;DATE_DATA_K_INDEX&quot;
   ON &quot;ERPI&quot;.&quot;DATE_DATA&quot;
   (
		&quot;DAYS_DATE&quot;
   );

CREATE INDEX &quot;DATE_DATA_NK_INDEX&quot;
   ON &quot;ERPI&quot;.&quot;DATE_DATA&quot;
   (
		&quot;TENANT&quot;,
		&quot;THE_DATE&quot;
   );

CREATE TABLE &quot;ERPI&quot;.&quot;DATE_DATA_EXTRACT&quot; 
(
	&quot;TENANT&quot;		VARCHAR(128) NOT NULL,
    &quot;DATA_MART&quot; 	VARCHAR(2) NOT NULL,
    &quot;THE_DATE&quot; 		DATE
);

CREATE VIEW &quot;ERPI&quot;.&quot;DATE_DATA_CL&quot; (
	 &quot;TENANT&quot;,
     &quot;DAYS_DATE&quot;,
     &quot;THE_DATE&quot;,
     &quot;YEAR&quot;,
     &quot;QUARTER&quot;,
     &quot;PERIOD&quot;,
	 &quot;MONTH&quot;,
	 &quot;WEEK&quot;,
     &quot;DAY&quot;
)
AS 
SELECT 
	&quot;TENANT&quot;,
    &quot;DAYS_DATE&quot;, 
	&quot;THE_DATE&quot;,
	&quot;YEAR&quot;,
	&quot;QUARTER&quot;, 
	&quot;PERIOD&quot;,
	&quot;MONTH&quot;,
	&quot;WEEK&quot;, 
	&quot;DAY&quot; 
FROM &quot;ERPI&quot;.&quot;DATE_DATA&quot; 
WHERE &quot;DATE_PATTERN&quot; = &apos;CL&apos;;

--
--  FISCAL_PERIODS is the time dimension at fiscal period level for 
--  period cubes (like general legder cubes)
--
CREATE TABLE &quot;ERPI&quot;.&quot;FISCAL_PERIODS&quot; 
(
	&quot;TENANT&quot;				VARCHAR(128) NOT NULL DEFAULT &apos;not used&apos;,
    &quot;FISCAL_PERIOD_K&quot;		INTEGER NOT NULL,
    &quot;FISCAL_VARIANT&quot;		VARCHAR(2),
    &quot;FISCAL_YEAR&quot;			SMALLINT,
    &quot;FISCAL_PERIOD&quot;			SMALLINT,
    &quot;END_DATE&quot;				DATE,
    &quot;END_DAYS_DATE&quot;			INTEGER,
    &quot;BEGIN_DATE&quot;			DATE,
    &quot;BEGIN_DAYS_DATE&quot;		INTEGER,
    &quot;FISCAL_PERIOD_K_END&quot; 	INTEGER,
    &quot;DATE_FROM&quot; 			${erpi.datamart.timestamp},
    &quot;DATE_TO&quot; 				${erpi.datamart.timestamp},
    &quot;VERSION&quot;				INTEGER
);

ALTER TABLE &quot;ERPI&quot;.&quot;FISCAL_PERIODS&quot;
ADD CONSTRAINT  &quot;FISCAL_PERIODS_PK&quot; PRIMARY KEY
    (
		&quot;TENANT&quot;,
        &quot;FISCAL_PERIOD_K&quot; 
    );

CREATE INDEX &quot;FISCAL_PERIODS_K_INDEX&quot;
   ON &quot;ERPI&quot;.&quot;FISCAL_PERIODS&quot;
   (
		&quot;FISCAL_PERIOD_K&quot;
   );

CREATE INDEX &quot;FISCAL_PERIODS_NK_INDEX&quot;
   ON &quot;ERPI&quot;.&quot;FISCAL_PERIODS&quot;
   (
		&quot;TENANT&quot;,
		&quot;FISCAL_YEAR&quot;,
		&quot;FISCAL_PERIOD&quot;
   );

CREATE VIEW &quot;ERPI&quot;.&quot;FISCAL_PERIODS_CL&quot; (
	&quot;TENANT&quot;,
    &quot;FISCAL_PERIOD_K&quot;,
    &quot;END_DAYS_DATE&quot;,
    &quot;FISCAL_VARIANT&quot;,
    &quot;FISCAL_YEAR&quot;,
    &quot;FISCAL_PERIOD&quot;,
    &quot;END_DATE&quot;,
    &quot;BEGIN_DAYS_DATE&quot;
 )
AS 
SELECT
	&quot;TENANT&quot;,
	&quot;FISCAL_PERIOD_K&quot;, 
	&quot;END_DAYS_DATE&quot;, 
	&quot;FISCAL_VARIANT&quot;,
	&quot;FISCAL_YEAR&quot;, 
	&quot;FISCAL_PERIOD&quot;,
	&quot;END_DATE&quot;, 
	&quot;BEGIN_DAYS_DATE&quot;
  FROM &quot;ERPI&quot;.&quot;FISCAL_PERIODS&quot; 
  WHERE (&quot;FISCAL_VARIANT&quot; = &apos;CL&apos; OR &quot;FISCAL_PERIOD_K&quot; = 0); 

CREATE TABLE &quot;ERPI&quot;.&quot;TEMP_FISCAL_PERIODS&quot; 
(
	&quot;TENANT&quot;				VARCHAR(128) NOT NULL DEFAULT &apos;not used&apos;,
    &quot;FISCAL_PERIOD_K&quot;		INTEGER NOT NULL,
    &quot;FISCAL_VARIANT&quot;		VARCHAR(2),
    &quot;FISCAL_YEAR&quot;			SMALLINT,
    &quot;FISCAL_PERIOD&quot;			SMALLINT,
    &quot;END_DATE&quot;				DATE,
    &quot;END_DAYS_DATE&quot;			INTEGER,
    &quot;BEGIN_DATE&quot;			DATE,
    &quot;BEGIN_DAYS_DATE&quot;		INTEGER,
    &quot;DATE_FROM&quot; 			${erpi.datamart.timestamp},
    &quot;DATE_TO&quot; 				${erpi.datamart.timestamp},
    &quot;VERSION&quot;				INTEGER
);

--
--  PERIODS is the time dimension at fiscal period level or monthly period
--  level depending on erpi.outbymonth.  It is used by outstanding (backlog) cubes. 
--

CREATE TABLE &quot;ERPI&quot;.&quot;PERIODS&quot; 
(
	&quot;TENANT&quot;				VARCHAR(128) NOT NULL DEFAULT &apos;not used&apos;,
    &quot;FISCAL_PERIOD_K&quot;		INTEGER NOT NULL,
    &quot;FISCAL_VARIANT&quot;		VARCHAR(2),
    &quot;FISCAL_YEAR&quot;			SMALLINT,
    &quot;FISCAL_PERIOD&quot;			SMALLINT,
    &quot;END_DATE&quot;				DATE,
    &quot;END_DAYS_DATE&quot;			INTEGER,
    &quot;BEGIN_DATE&quot;			DATE,
    &quot;BEGIN_DAYS_DATE&quot;		INTEGER,
    &quot;FISCAL_PERIOD_K_END&quot; 	INTEGER,
    &quot;DATE_FROM&quot; 			${erpi.datamart.timestamp},
    &quot;DATE_TO&quot; 				${erpi.datamart.timestamp},
    &quot;VERSION&quot;				INTEGER
);

ALTER TABLE &quot;ERPI&quot;.&quot;PERIODS&quot;
ADD CONSTRAINT  &quot;PERIODS_PK&quot; PRIMARY KEY
    (
		&quot;TENANT&quot;,
        &quot;FISCAL_PERIOD_K&quot; 
    );

CREATE INDEX &quot;PERIODS_K_INDEX&quot;
   ON &quot;ERPI&quot;.&quot;PERIODS&quot;
   (
		&quot;FISCAL_PERIOD_K&quot;
   );

CREATE INDEX &quot;PERIODS_NK_INDEX&quot;
   ON &quot;ERPI&quot;.&quot;PERIODS&quot;
   (
		&quot;TENANT&quot;,
		&quot;FISCAL_YEAR&quot;,
		&quot;FISCAL_PERIOD&quot;
   );

CREATE VIEW &quot;ERPI&quot;.&quot;PERIODS_CL&quot; (
	&quot;TENANT&quot;,
    &quot;FISCAL_PERIOD_K&quot;,
    &quot;END_DAYS_DATE&quot;,
    &quot;FISCAL_VARIANT&quot;,
    &quot;FISCAL_YEAR&quot;,
    &quot;FISCAL_PERIOD&quot;,
    &quot;END_DATE&quot;,
    &quot;BEGIN_DAYS_DATE&quot;
 )
AS 
SELECT
	&quot;TENANT&quot;,
	&quot;FISCAL_PERIOD_K&quot;, 
	&quot;END_DAYS_DATE&quot;, 
	&quot;FISCAL_VARIANT&quot;,
	&quot;FISCAL_YEAR&quot;, 
	&quot;FISCAL_PERIOD&quot;,
	&quot;END_DATE&quot;, 
	&quot;BEGIN_DAYS_DATE&quot;
  FROM &quot;ERPI&quot;.&quot;PERIODS&quot; 
  WHERE (&quot;FISCAL_VARIANT&quot; = &apos;CL&apos; OR &quot;FISCAL_PERIOD_K&quot; = 0); 

CREATE TABLE &quot;ERPI&quot;.&quot;TEMP_PERIODS&quot; 
(
	&quot;TENANT&quot;				VARCHAR(128) NOT NULL DEFAULT &apos;not used&apos;,
    &quot;FISCAL_PERIOD_K&quot;		INTEGER NOT NULL,
    &quot;FISCAL_VARIANT&quot;		VARCHAR(2),
    &quot;FISCAL_YEAR&quot;			SMALLINT,
    &quot;FISCAL_PERIOD&quot;			SMALLINT,
    &quot;END_DATE&quot;				DATE,
    &quot;END_DAYS_DATE&quot;			INTEGER,
    &quot;BEGIN_DATE&quot;			DATE,
    &quot;BEGIN_DAYS_DATE&quot;		INTEGER,
    &quot;DATE_FROM&quot; 			${erpi.datamart.timestamp},
    &quot;DATE_TO&quot; 				${erpi.datamart.timestamp},
    &quot;VERSION&quot;				INTEGER
);

