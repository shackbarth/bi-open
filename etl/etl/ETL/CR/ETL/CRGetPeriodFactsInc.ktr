  <step>
    <name>SALES_ORDER_FACTS &amp; SALES_DELIVERY_FACTS</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>erpi-jdbc</connection>
    <sql>SELECT	&quot;TENANT&quot;,
		&quot;DATE_PATTERN&quot;,
		&quot;FISCAL_PERIOD_K&quot; AS &quot;PERIOD_K&quot;,
		&quot;ID_DOCUMENT_TYPE&quot;,
		&quot;ID_DOCUMENT_NK&quot;, 
		&apos;order&apos; as &quot;TRANS_TYPE&quot;,
		&quot;ID_TRANS_DOC_NK&quot;, 
		&quot;ISSUE_DATE_K&quot;, 
       	&quot;PROMISE_DATE_K&quot;, 
       	0 as &quot;DELIVERY_DATE_K&quot;, 
		&quot;AMOUNT&quot;,
		&quot;AMOUNT_LOCAL&quot;, 
		&quot;COST&quot;,
		0.00 as &quot;ORDER_SUM&quot;,
		0.00 as &quot;ORDER_SUM_LOCAL&quot;,
		0.00 as &quot;DELIVERY_SUM&quot;,
		0.00 as &quot;DELIVERY_SUM_LOCAL&quot;,
		0.00 as &quot;COST_SUM&quot;, 
		&quot;COMPANY_K&quot;, 
		&quot;CUSTOMER_K&quot;, 
       	&quot;ITEM_K&quot;, 
		&quot;DIVISION_K&quot;, 
		&quot;BUSINESS_UNIT_K&quot;, 
		&quot;PROFIT_CENTER_K&quot;, 
		&quot;ACCOUNT_REP_K&quot;, 
       	&quot;LOCAL_CURRENCY_K&quot;,
		&quot;BILL_CITY_K&quot;,
		&quot;SHIP_CITY_K&quot;,
		&quot;FISCAL_PERIOD_K_END&quot; AS &quot;END_PERIOD&quot;
FROM &quot;ERPI&quot;.&quot;SALES_ORDER_FACTS&quot;

LEFT OUTER JOIN

(SELECT 	&quot;FISCAL_PERIOD_K&quot;, 
			datedata.&quot;DAYS_DATE&quot;, 
			&quot;DATE_PATTERN&quot;,
			&quot;FISCAL_YEAR&quot;,
			&quot;FISCAL_PERIOD&quot;,
			&quot;FISCAL_PERIOD_K_END&quot;
FROM &quot;ERPI&quot;.&quot;PERIODS&quot;, &quot;ERPI&quot;.&quot;DATE_DATA&quot; as datedata
WHERE ( &quot;FISCAL_YEAR&quot; = &quot;YEAR&quot; and &quot;FISCAL_PERIOD&quot; = ${monthorperiodcolumn} and &quot;ERPI&quot;.&quot;PERIODS&quot;.&quot;TENANT&quot; = &apos;${erpi.tenant.id}&apos;)
) periods1
ON &quot;ISSUE_DATE_K&quot; = &quot;DAYS_DATE&quot;

WHERE &quot;FISCAL_PERIOD_K&quot; &gt;= ${extractperiod}
		AND &quot;TENANT&quot; = &apos;${erpi.tenant.id}&apos;

UNION

SELECT	&quot;TENANT&quot;,
		&quot;DATE_PATTERN&quot;,
		&quot;FISCAL_PERIOD_K&quot; AS &quot;PERIOD_K&quot;,
		&quot;ID_DOCUMENT_TYPE&quot;,
		&quot;ID_DOCUMENT_NK&quot;,
		&apos;delivery&apos; as &quot;TRANS_TYPE&quot;,
		&quot;ID_TRANS_DOC_NK&quot;, 
		&quot;ISSUE_DATE_K&quot;, 
       	&quot;PROMISE_DATE_K&quot;, 
       	0 as &quot;DELIVERY_DATE_K&quot;, 
		&quot;AMOUNT&quot;,
		&quot;AMOUNT_LOCAL&quot;, 
		&quot;COST&quot;,
		0.00 as &quot;ORDER_SUM&quot;,
		0.00 as &quot;ORDER_SUM_LOCAL&quot;,
		0.00 as &quot;DELIVERY_SUM&quot;,
		0.00 as &quot;DELIVERY_SUM_LOCAL&quot;,
		0 as &quot;COST_SUM&quot;, 
		&quot;COMPANY_K&quot;, 
		&quot;CUSTOMER_K&quot;, 
       	&quot;ITEM_K&quot;, 
		&quot;DIVISION_K&quot;, 
		&quot;BUSINESS_UNIT_K&quot;, 
		&quot;PROFIT_CENTER_K&quot;, 
		&quot;ACCOUNT_REP_K&quot;, 
       	&quot;LOCAL_CURRENCY_K&quot;,
		&quot;BILL_CITY_K&quot;,
		&quot;SHIP_CITY_K&quot;,
		&quot;FISCAL_PERIOD_K_END&quot; AS &quot;END_PERIOD&quot;
FROM &quot;ERPI&quot;.&quot;SALES_DELIVERY_FACTS&quot;

LEFT OUTER JOIN

(SELECT 	&quot;FISCAL_PERIOD_K&quot;, 
			datedata.&quot;DAYS_DATE&quot;, 
			&quot;DATE_PATTERN&quot;,
			&quot;FISCAL_YEAR&quot;,
