  <step>
    <name>Generate period fact rows</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <compatible>N</compatible>
    <optimizationLevel>9</optimizationLevel>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>ScriptValue</jsScript_name>
        <jsScript_script>&#47;&#47;
&#47;&#47;	Write the cache.  If any documents have a 0 balance, remove them.  Write the 
&#47;&#47;	cache for each period until toTime.
&#47;&#47;
function writeCache(fromTime, toTime)
{
	if (fromTime == toTime)
	{
		return;
	}

	newCache = [];

	for (var i=0; i &lt; cache.length; i++) 
	{
		putRow(cache[i]);
	}

	fromTime++;

	&#47;&#47;
	&#47;&#47; After we write the cache we increment time to next period.  Before we can 
	&#47;&#47; write again we need to create new row objects and copy in info from the cache.
	&#47;&#47; If not, the put will just put references to rows.
	&#47;&#47;
	for (var i=0; i &lt; cache.length; i++)
	{
			newRow = createRowCopy(getInputRowMeta().size());
			newCache.push(newRow);
			newCache[i][0] = String(cache[i][0]);
			newCache[i][1] = fromTime;
			newCache[i][2] = String(cache[i][2]);
			newCache[i][4] = String(cache[i][4]);
			for (j = 5; j &lt; 25; j++)
			{
				newCache[i][j] = Number(cache[i][j]);			
			}
	}

	cache = [];

	&#47;&#47;
	&#47;&#47; For the next write we use orders where order sum is greater than the delivery sum.
	&#47;&#47; These are the &quot;open&quot; orders.
	&#47;&#47;

	for (var i=0; i &lt; newCache.length; i++) 
	{
		if (newCache[i][11] &gt; newCache[i][13])
		{
			cache.push(newCache[i]);
		}
	}

	writeCache(fromTime, toTime);

}

&#47;&#47;
&#47;&#47;	If this is the first row, set the current time, end time and save fiscal pattern
&#47;&#47;
if(getProcessCount(&quot;r&quot;) == 1) 
{
	var currTime = new Number(row[1]);
	var endTime = new Number(END_PERIOD);
	var cache = new Array();
	var fiscalCode = row[0];
}

&#47;&#47;
&#47;&#47;	Retrieve the time from the row.
&#47;&#47;
rowTime = Number(row[1]);

&#47;&#47;
&#47;&#47;	Check rows are sorted and in range of erpi.endate
&#47;&#47;
if (rowTime &lt; currTime || rowTime &gt; endTime)
{
	trans_Status = ERROR_TRANSFORMATION;
	writeToLog(&quot;row not sorted by ascending year &amp; period or outside range of erpi.fiscal_period_k&quot;);
}

&#47;&#47;
&#47;&#47;	If the period changed, write the cache up to the new time.
&#47;&#47; 
if (currTime != rowTime)
{
	writeCache(currTime, rowTime);
}
currTime = rowTime;

&#47;&#47;
&#47;&#47;	If the document does not exist in the cache, insert it.  If it does, update.
&#47;&#47;
for (var i=0; i &lt;= cache.length; i++) 
{
		if ((i == cache.length) || (cache[i][2] == row[2]))
		{
			if (i == cache.length)
			{
				cache.push(row);
			}
			if ( row[3] == &quot;order&quot;)
			{
				cache[i][5] = row[5];  &#47;&#47;issue_date always order date
				cache[i][11] = str2num(cache[i][11]) + str2num(row[8]); &#47;&#47;order_sum
				cache[i][12] = str2num(cache[i][12]) + str2num(row[9]); &#47;&#47;order_sum_local
			}
			else
			{
				cache[i][7] = row[5];  &#47;&#47;delivery_date is last delivery date
				cache[i][13] = str2num(cache[i][13]) + str2num(row[8]); &#47;&#47;delivery_sum
				cache[i][14] = str2num(cache[i][14]) + str2num(row[9]); &#47;&#47;delivery_sum_local
				cache[i][15] = Number(cache[i][15]) + Number(row[10]); &#47;&#47;cost_sum
			}
			break;
		}
}
&#47;&#47;
&#47;&#47;	If this is the last row, or the fiscal code changes write the cache through erpi.endate.  
&#47;&#47;  If the fiscal code changes start over again at the begining of time with a new cache
&#47;&#47;
if ((row[26] == true) || (row[0] != fiscalCode))
{
	writeCache(currTime, endTime + 1);
	var currTime = Number(row[1]);
	var endTime = new Number(END_PERIOD);
	var cache = new Array();
	var fiscalCode = row[0];
}

&#47;&#47;
&#47;&#47;	Discard this row.  We write our own rows using putRow
&#47;&#47;
trans_Status = SKIP_TRANSFORMATION;

</jsScript_script>
