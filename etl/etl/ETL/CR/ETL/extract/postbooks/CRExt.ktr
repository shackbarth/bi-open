  <step>
    <name>Extract QuoteTransactions</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>source</connection>
    <sql>select
	&apos;${erpi.tenant.id}&apos;																		as &quot;Tenant&quot;,
	CASE WHEN quhead_number = &apos;&apos; THEN &apos;Unspecified&apos;
		ELSE COALESCE(quhead_number, &apos;Unspecified&apos;) END 									as &quot;ID_Document_NK&quot;,
	CASE WHEN cust_name = &apos;&apos; THEN &apos;Unspecified&apos;
		ELSE COALESCE(cust_name, &apos;Unspecified&apos;) END 									as &quot;ID_Document_Name&quot;,
	CASE WHEN quhead_number = &apos;&apos; THEN &apos;Unspecified&apos;
		ELSE COALESCE(quhead_number, &apos;Unspecified&apos;) END 									as &quot;ID_Trans_Doc_NK&quot;, 	
	(CASE WHEN quhead_number = &apos;&apos; THEN &apos;Unspecified&apos; 
		ELSE COALESCE(CAST(quhead_number as varchar(30)), &apos;Unspecified&apos;) END) || &apos;-&apos; ||	quitem.quitem_id::character varying	as &quot;Line_NK&quot;,
	CASE WHEN quhead_status = &apos;&apos; THEN &apos;Unspecified&apos;
         WHEN quhead_status = &apos;O&apos; THEN &apos;Open&apos;
         WHEN quhead_status = &apos;C&apos; THEN &apos;Converted&apos;
		ELSE COALESCE(quhead_status, &apos;Unspecified&apos;) END										as &quot;Status&quot;, 
	to_char(quhead_quotedate,&apos;MM&#47;DD&#47;YYYY&apos;)													as &quot;Issue_Date&quot;,
--
--  We can&apos;t allow null or nonsense promise dates.  An no earlier dates as they may not exist in the dates dimension
-- 													
    CASE
            WHEN quitem_promdate IS NULL THEN to_char(quhead_quotedate,&apos;MM&#47;DD&#47;YYYY&apos;)::text
            WHEN quitem_promdate &lt; quhead_quotedate THEN to_char(quhead_quotedate,&apos;MM&#47;DD&#47;YYYY&apos;)::text
            WHEN to_char(quitem_promdate,&apos;MM&#47;DD&#47;YYYY&apos;) = &apos;01&#47;01&#47;2100&apos;::bpchar THEN to_char(quhead_quotedate,&apos;MM&#47;DD&#47;YYYY&apos;)::text
            ELSE to_char(quitem_promdate,&apos;MM&#47;DD&#47;YYYY&apos;)::text
    END 																					as &quot;Promise_Date&quot;,

--    CASE
--            WHEN quhead_expire IS NULL THEN COALESCE(to_char(quhead_expire,&apos;MM&#47;DD&#47;YYYY&apos;), &apos;01&#47;01&#47;1990&apos;)
--            ELSE to_char(quhead_expire,&apos;MM&#47;DD&#47;YYYY&apos;)::text
--    END																						as &quot;Expire_Date&quot;,

	to_char(quhead_expire,&apos;MM&#47;DD&#47;YYYY&apos;)::text												as &quot;Expire_Date&quot;,

	currtobase(quhead_curr_id, quitem_price, quhead_quotedate) 								as so_baseunitprice,
	round((quitem_qtyord * currtobase(quhead_curr_id, quitem_custprice, quhead_quotedate)), 2) 	as &quot;Amount_List&quot;,
	round((quitem_qtyord * currtobase(quhead_curr_id, quitem_custprice, quhead_quotedate)), 2) 	as &quot;Amount_List_Local&quot;,
	round((quitem_qtyord * currtobase(quhead_curr_id, quitem_price, quhead_quotedate)), 2) 	as &quot;Amount&quot;,
	round((quitem_qtyord * currtobase(quhead_curr_id, quitem_price, quhead_quotedate)), 2) 	as &quot;Amount_Local&quot;,
	quitem_qtyord																			as &quot;Quantity_Order&quot;,					    
	COALESCE(quitem_unitcost, 0)															as &quot;Cost&quot;,
	cust_number 																			as &quot;Customer&quot;,
	(SELECT company_number FROM company ORDER BY company_number DESC LIMIT 1)				as &quot;Company&quot;,
	item_number																				as &quot;Item&quot;,
	(SELECT curr_abbr FROM curr_symbol WHERE curr_base = TRUE) 								as &quot;Local_Currency&quot;,
	&apos;Unspecified&apos; 																			as &quot;Division&quot;,
	&apos;Unspecified&apos; 																			as &quot;Business_Unit&quot;,
	salesrep_number 																		as &quot;Account_Rep&quot;,
	&apos;Unspecified&apos; 																			as &quot;Profit_Center&quot;,
	CASE WHEN quhead_shiptocity = &apos;&apos; THEN &apos;Unspecified&apos;
		ELSE COALESCE(upper(regexp_replace(quhead_shiptocity, E&apos;\r|\n|;&apos;, &apos;&apos;, &apos;g&apos;)), &apos;Unspecified&apos;) 
		END																					as &quot;Ship_City&quot;,
	CASE WHEN quhead_shiptostate = &apos;&apos; THEN &apos;Unspecified&apos;
		ELSE COALESCE(upper(regexp_replace(quhead_shiptostate, E&apos;\r|\n|;&apos;, &apos;&apos;, &apos;g&apos;)), &apos;Unspecified&apos;) 
		END																					as &quot;Ship_Region&quot;,
	CASE WHEN quhead_shiptocountry = &apos;&apos; THEN &apos;${erpi.defaultcountry}&apos;
		ELSE COALESCE(upper(regexp_replace(quhead_shiptocountry, E&apos;\r|\n|;&apos;, &apos;&apos;, &apos;g&apos;)), &apos;${erpi.defaultcountry}&apos;) 
		END																					as &quot;Ship_Country&quot;,
	CASE WHEN quhead_billtocity = &apos;&apos; THEN &apos;Unspecified&apos;
		ELSE COALESCE(upper(regexp_replace(quhead_billtocity, E&apos;\r|\n|;&apos;, &apos;&apos;, &apos;g&apos;)), &apos;Unspecified&apos;) 
		END																					as &quot;Bill_City&quot;,
	CASE WHEN quhead_billtostate = &apos;&apos; THEN &apos;Unspecified&apos;
		ELSE COALESCE(upper(regexp_replace(quhead_billtostate, E&apos;\r|\n|;&apos;, &apos;&apos;, &apos;g&apos;)), &apos;Unspecified&apos;) 
		END							as &quot;Bill_Region&quot;,
	CASE WHEN quhead_billtocountry = &apos;&apos; THEN &apos;${erpi.defaultcountry}&apos;
		ELSE COALESCE(upper(regexp_replace(quhead_billtocountry, E&apos;\r|\n|;&apos;, &apos;&apos;, &apos;g&apos;)), &apos;${erpi.defaultcountry}&apos;) 
		END			as &quot;Bill_Country&quot;
from quitem
 join quhead on quitem_quhead_id=quhead_id													-- sales order 
  join custinfo on cust_id=quhead_cust_id													-- customer 
  join salesrep on salesrep_id=quhead_salesrep_id											-- sales rep
  join itemsite on itemsite_id=quitem_itemsite_id											-- item inventory site
  join whsinfo on warehous_id=itemsite_warehous_id											-- warehouse &#47; site
  join item on item_id=itemsite_item_id

WHERE (quhead_quotedate &gt;= &apos;${extractstartdate}&apos; OR (quhead_quotedate &gt;= &apos;${erpi.start.date}&apos;))
 	-- we need a lastupdated column in quotes.
 	-- AND quhead_lastupdated &gt;= &apos;${extractstartdate}&apos;))
</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
