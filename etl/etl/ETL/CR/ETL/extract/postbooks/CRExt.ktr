  <step>
    <name>Set status and actual date.</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <compatible>Y</compatible>
    <optimizationLevel>9</optimizationLevel>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47;
&#47;&#47;  Clean opportunities more (after SQL select clean).  If stage is in closed list, 
&#47;&#47;  make status closed.  We need to know which are won so if in won list make the 
&#47;&#47;  status won.  If closed or won, make the actual date the last date set.
&#47;&#47;

var closedStages = getVariable(&quot;erpi.opportunity.closed&quot;, &quot;&quot;),
  closedStagesList = closedStages.split(&quot;,&quot;),
  wonStages = getVariable(&quot;erpi.opportunity.won&quot;, &quot;&quot;),
  wonStagesList = wonStages.split(&quot;,&quot;);

for (i = 0; i &lt; closedStagesList.length; i++) {
  if ( Stage.getString() == closedStagesList[i]) {
    Status.setValue(&quot;Closed&quot;);
  }
}

for (i = 0; i &lt; wonStagesList.length; i++) {
  if ( Stage.getString() == wonStagesList[i]) {
    Status.setValue(&quot;Won&quot;);
  }
}

if ( (Status.getString() == &quot;Closed&quot;) || (Status.getString() == &quot;Won&quot;) ) {
	if ( Assign_Date.getString() == &quot;01&#47;01&#47;1990&quot; ) {
		Assign_Date.setValue(Issue_Date.getString());
	}
	if ( Target_Date.getString() == &quot;01&#47;01&#47;1990&quot; ) {
		Target_Date.setValue(Assign_Date.getString());
	}
	if ( Actual_Date.getString() == &quot;01&#47;01&#47;1990&quot; ) {
		Actual_Date.setValue(Target_Date.getString());
	}
}</jsScript_script>
