  <step>
    <name>Extract Opportunity Transactions</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>source</connection>
    <sql>--
--  The start date is not required in opportunities so we can&apos;t extract based on date range (arg!)
--  Also, there is no lastupdated date for incremental updates.
--
--  To clean dates:
--		A null date is 01&#47;01&#47;1990.
--		If a date is null and later phase date is not, we use it.
--      The start date can never be null, we use current_date instead.
--
select
	&apos;${erpi.tenant.id}&apos;															as &quot;Tenant&quot;,
	CASE WHEN ophead_number = &apos;&apos; THEN &apos;Unspecified&apos;
		ELSE COALESCE(ophead_number, &apos;Unspecified&apos;) END 									as &quot;ID_Document_NK&quot;,
	CASE WHEN ophead_name = &apos;&apos; THEN &apos;Unspecified&apos;
		ELSE COALESCE(ophead_name, &apos;Unspecified&apos;) END 									as &quot;ID_Document_Name&quot;,
	CASE WHEN ophead_number = &apos;&apos; THEN &apos;Unspecified&apos;
		ELSE COALESCE(ophead_number, &apos;Unspecified&apos;) END 									as &quot;ID_Trans_Doc_NK&quot;,
	CASE WHEN ophead_active = true THEN &apos;Active&apos;
		ELSE &apos;Closed&apos; END
														as &quot;Status&quot;, 
	COALESCE(to_char(ophead_actual_date,&apos;MM&#47;DD&#47;YYYY&apos;), &apos;01&#47;01&#47;1990&apos;)			as &quot;Actual_Date&quot;,

    CASE
            WHEN ophead_target_date IS NULL THEN COALESCE(to_char(ophead_actual_date,&apos;MM&#47;DD&#47;YYYY&apos;), &apos;01&#47;01&#47;1990&apos;)
            ELSE to_char(ophead_target_date,&apos;MM&#47;DD&#47;YYYY&apos;)::text
    END																			as &quot;Target_Date&quot;,

    CASE
            WHEN ophead_assigned_date IS NULL THEN
				CASE
					WHEN ophead_target_date IS NULL THEN COALESCE(to_char(ophead_actual_date,&apos;MM&#47;DD&#47;YYYY&apos;), &apos;01&#47;01&#47;1990&apos;)
					ELSE to_char(ophead_target_date,&apos;MM&#47;DD&#47;YYYY&apos;)::text
				END
            ELSE to_char(ophead_assigned_date,&apos;MM&#47;DD&#47;YYYY&apos;)::text
	END																			as &quot;Assign_Date&quot;,			

    CASE
            WHEN ophead_start_date IS NULL THEN

				CASE
					WHEN ophead_assigned_date IS NULL THEN

						CASE
							WHEN ophead_target_date IS NULL THEN COALESCE(to_char(ophead_actual_date,&apos;MM&#47;DD&#47;YYYY&apos;), to_char(CURRENT_DATE,&apos;MM&#47;DD&#47;YYYY&apos;))
							ELSE to_char(ophead_target_date,&apos;MM&#47;DD&#47;YYYY&apos;)::text
						END
					ELSE to_char(ophead_assigned_date,&apos;MM&#47;DD&#47;YYYY&apos;)::text
				END


            ELSE to_char(ophead_start_date,&apos;MM&#47;DD&#47;YYYY&apos;)::text					
	END																			as &quot;Issue_Date&quot;,	

 	COALESCE(ophead_amount, 0)												 	as &quot;Amount&quot;,
	COALESCE(ophead_amount, 0)												 	as &quot;Amount_Local&quot;,
 	COALESCE(ophead_amount, 0) * COALESCE(ophead_probability_prcnt, 0) &#47; 100	as &quot;Amount_Weighted&quot;,
 	COALESCE(ophead_amount, 0) * COALESCE(ophead_probability_prcnt, 0) &#47; 100	as &quot;Amount_Local_Weighted&quot;,
	COALESCE(ophead_probability_prcnt, 0)									 	as &quot;Probability&quot;,
	(SELECT company_number FROM company ORDER BY company_number DESC LIMIT 1)	as &quot;Company&quot;,
	(SELECT curr_abbr FROM curr_symbol WHERE curr_base = TRUE) 					as &quot;Local_Currency&quot;,
	&apos;Unspecified&apos; 																as &quot;Division&quot;,
	&apos;Unspecified&apos; 																as &quot;Business_Unit&quot;,
	CASE WHEN ophead_username = &apos;&apos; THEN &apos;Unspecified&apos;
		ELSE COALESCE(ophead_username, &apos;Unspecified&apos;) END						as &quot;User&quot;,
	&apos;Unspecified&apos; 																as &quot;Profit_Center&quot;,
	optype_name																	as &quot;Type&quot;,
	opstage_name																as &quot;Stage&quot;,
	crmacct_number																as &quot;CRM_Account&quot;,
	CASE WHEN addr_city = &apos;&apos; THEN &apos;Unspecified&apos;
		ELSE COALESCE(upper(regexp_replace(addr_city, E&apos;\r|\n|;&apos;, &apos;&apos;, &apos;g&apos;)), &apos;Unspecified&apos;) 
		END																					as &quot;Contact_City&quot;,
	CASE WHEN addr_state = &apos;&apos; THEN &apos;Unspecified&apos;
		ELSE COALESCE(upper(regexp_replace(addr_state, E&apos;\r|\n|;&apos;, &apos;&apos;, &apos;g&apos;)), &apos;Unspecified&apos;) 
		END																					as &quot;Contact_Region&quot;,
	CASE WHEN addr_country = &apos;&apos; THEN &apos;${erpi.defaultcountry}&apos;
		ELSE COALESCE(upper(regexp_replace(addr_country, E&apos;\r|\n|;&apos;, &apos;&apos;, &apos;g&apos;)), &apos;${erpi.defaultcountry}&apos;) 
		END																					as &quot;Contact_Country&quot;

from ophead
  join optype on ophead_optype_id = optype_id									-- opportunity type
  join opstage on ophead_opstage_id = opstage_id								-- opportunity stage
  join crmacct on ophead_crmacct_id = crmacct_id								-- crm account
  join cntct on crmacct_cntct_id_1 = cntct_id									-- contact 1
  join addr on cntct_addr_id = addr_id											-- address


	--WHERE (ophead_start_date &gt;= &apos;${extractstartdate}&apos; )
 	-- AND ophead_lastupdated &gt;= &apos;${extractstartdate}&apos;))
</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
     <cluster_schema/>
 <remotesteps>   <input>   </input>   <output>   </output> </remotesteps>    <GUI>
