    <sql>SELECT 	&quot;ISS_DATE_PATTERN&quot;,
		&quot;ISS_FISCAL_PERIOD_K&quot;,  
       	&quot;ACT_FISCAL_PERIOD_K&quot;, 
		&quot;ENDDATE_K&quot;, 
		&quot;ID_DOCUMENT_NK&quot;,
        &quot;ID_DOCUMENT_NAME&quot;,  
       	&quot;ID_TRANS_DOC_NK&quot;, 
		&quot;STATUS&quot;, 
		&quot;TYPE&quot;, 
		&quot;STAGE&quot;, 
		&quot;ISSUE_DATE_K&quot;, 
       	&quot;ASSIGN_DATE_K&quot;, 
		&quot;TARGET_DATE_K&quot;, 		
		&quot;ACTUAL_DATE_K&quot;, 	
		&quot;AMOUNT&quot;, 
       	&quot;AMOUNT_LOCAL&quot;, 
		&quot;AMOUNT_WEIGHTED&quot;, 
		&quot;AMOUNT_LOCAL_WEIGHTED&quot;, 
		&quot;PROBABILITY&quot;, 
       	&quot;COMPANY_K&quot;, 
		&quot;DIVISION_K&quot;, 
		&quot;BUSINESS_UNIT_K&quot;, 
		&quot;PROFIT_CENTER_K&quot;, 
       	&quot;CRM_ACCOUNT_K&quot;, 
		&quot;USER_K&quot;, 
		&quot;LOCAL_CURRENCY_K&quot;,
		&quot;CONTACT_CITY_K&quot;
FROM 	&quot;ERPI&quot;.&quot;CRM_OPPORTUNITY_FACTS&quot;

LEFT OUTER JOIN
(SELECT 	&quot;FISCAL_PERIOD_K&quot; as &quot;ISS_FISCAL_PERIOD_K&quot;, 
			datedata.&quot;DAYS_DATE&quot; as &quot;ISS_DAYS_DATE&quot;, 
			&quot;DATE_PATTERN&quot; AS &quot;ISS_DATE_PATTERN&quot;,
			&quot;FISCAL_YEAR&quot;,
			&quot;FISCAL_PERIOD&quot;,
			&quot;FISCAL_PERIOD_K_END&quot; as &quot;ENDDATE_K&quot;
FROM &quot;ERPI&quot;.&quot;PERIODS&quot;, &quot;ERPI&quot;.&quot;DATE_DATA&quot; as datedata
WHERE ( &quot;FISCAL_YEAR&quot; = &quot;YEAR&quot; and 
        &quot;FISCAL_PERIOD&quot; = ${monthorperiodcolumn} and 
        &quot;ERPI&quot;.&quot;PERIODS&quot;.&quot;TENANT&quot; = &apos;${erpi.tenant.id}&apos; and
        datedata.&quot;TENANT&quot; = &apos;${erpi.tenant.id}&apos;)
) periods1
ON &quot;ISSUE_DATE_K&quot; = &quot;ISS_DAYS_DATE&quot;

LEFT OUTER JOIN
(SELECT 	&quot;FISCAL_PERIOD_K&quot; as &quot;ACT_FISCAL_PERIOD_K&quot;, 
			datedata2.&quot;DAYS_DATE&quot; AS&quot;ACT_DAYS_DATE&quot;, 
			&quot;DATE_PATTERN&quot;,
			&quot;FISCAL_YEAR&quot;,
			&quot;FISCAL_PERIOD&quot;
FROM &quot;ERPI&quot;.&quot;PERIODS&quot;, &quot;ERPI&quot;.&quot;DATE_DATA&quot; as datedata2
WHERE ( &quot;FISCAL_YEAR&quot; = &quot;YEAR&quot; and 
        &quot;FISCAL_PERIOD&quot; = ${monthorperiodcolumn} and 
        &quot;ERPI&quot;.&quot;PERIODS&quot;.&quot;TENANT&quot; = &apos;${erpi.tenant.id}&apos; and
        datedata2.&quot;TENANT&quot; = &apos;${erpi.tenant.id}&apos;)
) periods2
ON &quot;ACTUAL_DATE_K&quot; = &quot;ACT_DAYS_DATE&quot;

WHERE &quot;TENANT&quot; = &apos;${erpi.tenant.id}&apos;

ORDER By 1, 2, 3</sql>
