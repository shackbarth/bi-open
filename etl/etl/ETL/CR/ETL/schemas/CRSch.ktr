    REFERENCES &quot;ERPI&quot;.&quot;BUSINESS_UNIT&quot;(&quot;TENANT&quot;,&quot;BUSINESS_UNIT_K&quot;);

ALTER TABLE &quot;ERPI&quot;.&quot;CRM_QUOTE_FACTS&quot;
