ALTER TABLE &quot;ERPI&quot;.&quot;CRM_QUOTE_FACTS&quot;
ADD CONSTRAINT &quot;CRM_QUOTE_SHIP_CITY_FK&quot; FOREIGN KEY (&quot;TENANT&quot;,&quot;SHIP_CITY_K&quot;)
    REFERENCES &quot;ERPI&quot;.&quot;GEO_CITIES_DATA&quot;(&quot;TENANT&quot;,&quot;GEO_DATA_K&quot;);

