	&quot;ACTUAL_FISCAL_PERIOD_K&quot; 	INTEGER NOT NULL,
	&quot;ENDDATE_K&quot; 				INTEGER NOT NULL,
    &quot;ID_DOCUMENT_NK&quot; 			VARCHAR(128) NOT NULL DEFAULT &apos;Unspecified&apos;,		&#47;* opportunity document - degenerate dimension *&#47;
    &quot;ID_DOCUMENT_NAME&quot; 			VARCHAR(128) NOT NULL DEFAULT &apos;Unspecified&apos;,		&#47;* opportunity document name - degenerate dimension *&#47;
    &quot;ID_TRANS_DOC_NK&quot; 			VARCHAR(128) NOT NULL DEFAULT &apos;Unspecified&apos;,		&#47;* placeholder for lead document - degenerate *&#47;	
