******************************************************************&#47; 
INSERT INTO &quot;ERPI&quot;.&quot;AGG_NOITEM_QUOTE_FACTS&quot; (
	&quot;TENANT&quot;,
    &quot;ORDERDATE_YEAR&quot;,
    &quot;ORDERDATE_PERIOD&quot;,
    &quot;PROMISEDATE_YEAR&quot;,
    &quot;PROMISEDATE_PERIOD&quot;,
	&quot;STATUS&quot;,
    &quot;COMPANY_K&quot;,
    &quot;CUSTOMER_K&quot;,
    &quot;DIVISION_K&quot;,
    &quot;BUSINESS_UNIT_K&quot;,
    &quot;PROFIT_CENTER_K&quot;,
    &quot;ACCOUNT_REP_K&quot;,
    &quot;BILL_CITY_K&quot;,
    &quot;SHIP_CITY_K&quot;,
    &quot;LOCAL_CURRENCY_K&quot;,
    &quot;AMOUNT&quot;,
    &quot;AMOUNT_LOCAL&quot;,
    &quot;AMOUNT_LIST&quot;,
    &quot;AMOUNT_LIST_LOCAL&quot;,
    &quot;QUANTITY_ORDER&quot;,
    &quot;ID_DOCUMENT_NK&quot;,
    &quot;COST&quot;,
    &quot;DAYS_ORDER_DATE&quot;,
    &quot;DAYS_PROMISE_DATE&quot;,
    &quot;FACT_COUNT&quot;
    )
select
	&quot;QUOTE_FACTS&quot;.&quot;TENANT&quot;,
    &quot;ISSUE_DATE&quot;.&quot;YEAR&quot; as &quot;ORDERDATE_YEAR&quot;,
    &quot;ISSUE_DATE&quot;.&quot;PERIOD&quot; as &quot;ORDERDATE_PERIOD&quot;,
    &quot;PROMISE_DATE&quot;.&quot;YEAR&quot; as &quot;PROMISEDATE_YEAR&quot;,
    &quot;PROMISE_DATE&quot;.&quot;PERIOD&quot; as &quot;PROMISEDATE_PERIOD&quot;,
	&quot;STATUS&quot;,
    &quot;COMPANY_K&quot;,
    &quot;CUSTOMER_K&quot;,
    &quot;DIVISION_K&quot;,
    &quot;BUSINESS_UNIT_K&quot;,
    &quot;PROFIT_CENTER_K&quot;,
    &quot;ACCOUNT_REP_K&quot;,
    &quot;BILL_CITY_K&quot;,
    &quot;SHIP_CITY_K&quot;,
    &quot;LOCAL_CURRENCY_K&quot;,
    sum(&quot;QUOTE_FACTS&quot;.&quot;AMOUNT&quot;) as &quot;AMOUNT&quot;,
    sum(&quot;QUOTE_FACTS&quot;.&quot;AMOUNT_LOCAL&quot;) as &quot;AMOUNT_LOCAL&quot;,
    sum(&quot;QUOTE_FACTS&quot;.&quot;AMOUNT_LIST&quot;) as &quot;AMOUNT_LIST&quot;,
    sum(&quot;QUOTE_FACTS&quot;.&quot;AMOUNT_LIST_LOCAL&quot;) as &quot;AMOUNT_LIST_LOCAL&quot;,
    sum(&quot;QUOTE_FACTS&quot;.&quot;QUANTITY_ORDER&quot;) as &quot;QUANTITY_ORDER&quot;,
    count(distinct &quot;QUOTE_FACTS&quot;.&quot;ID_DOCUMENT_NK&quot;) as &quot;ID_DOCUMENT_NK&quot;,
    sum(&quot;QUOTE_FACTS&quot;.&quot;COST&quot;) as &quot;COST&quot;,
    min(&quot;QUOTE_FACTS&quot;.&quot;ISSUE_DATE_K&quot;) as &quot;DAYS_ORDER_DATE&quot;,
    max(&quot;QUOTE_FACTS&quot;.&quot;PROMISE_DATE_K&quot;) as &quot;DAYS_PROMISE_DATE&quot;,   
    count(*) as &quot;FACT_COUNT&quot;
from
    &quot;ERPI&quot;.&quot;CRM_QUOTE_FACTS&quot; as &quot;QUOTE_FACTS&quot;,
    &quot;ERPI&quot;.&quot;DATE_DATA_CL&quot; as &quot;ISSUE_DATE&quot;,
    &quot;ERPI&quot;.&quot;DATE_DATA_CL&quot; as &quot;PROMISE_DATE&quot;
where
    &quot;QUOTE_FACTS&quot;.&quot;ISSUE_DATE_K&quot; = &quot;ISSUE_DATE&quot;.&quot;DAYS_DATE&quot;
and
    &quot;QUOTE_FACTS&quot;.&quot;PROMISE_DATE_K&quot; = &quot;PROMISE_DATE&quot;.&quot;DAYS_DATE&quot;
group by
	&quot;QUOTE_FACTS&quot;.&quot;TENANT&quot;,
    &quot;ISSUE_DATE&quot;.&quot;YEAR&quot;,
    &quot;ISSUE_DATE&quot;.&quot;PERIOD&quot;,
    &quot;PROMISE_DATE&quot;.&quot;YEAR&quot;,
    &quot;PROMISE_DATE&quot;.&quot;PERIOD&quot;,
	&quot;STATUS&quot;,
    &quot;COMPANY_K&quot;,
    &quot;CUSTOMER_K&quot;,
    &quot;DIVISION_K&quot;,
    &quot;BUSINESS_UNIT_K&quot;,
    &quot;PROFIT_CENTER_K&quot;,
    &quot;ACCOUNT_REP_K&quot;,
    &quot;BILL_CITY_K&quot;,
    &quot;SHIP_CITY_K&quot;,
    &quot;LOCAL_CURRENCY_K&quot;;
